{
  "name": "coopeenortol",
  "version": "2.0.0",
  "description": "Plataforma de gestión cooperativa - Coopeenortol",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-deps": "npm install && cd server && npm install && cd ../client && npm install",
    "install-vps": "chmod +x scripts/install-ubuntu-22.04.sh && sudo scripts/install-ubuntu-22.04.sh"
  },
  "keywords": ["cooperativa", "asociados", "finanzas", "gestión", "coopeenortol"],
  "author": "Coopeenortol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coopeenortol/plataforma.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}